{
	"name": "Potential Barnacle",
	"description": "Batch testing for website filters",
	"default_locale": "en",
	"version": "0.0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": ["dns","notifications","socket","background"],
	"short_name": "PBarnacle",
	"sockets": {
		"tcp": {
			"connect": "*",
		},
		"udp": {
			"connect": "*",
		}
	}
}
